{
  "name": "sfgmedicare-extras",
  "version": "0.1.0",
  "main": "Gruntfile.js",
  "author": "TheWebist",
  "scripts": {
    "build": "npm run sass:prod",
    "sass:prod": "sass --no-source-map --style=compressed lib/scss:lib/dist",
    "sass:dev": "sass lib/scss:lib/css",
    "watch": "onchange \"lib/scss/**/*.scss\" -- npm run sass:dev",
    "readme": "npx wp-readme-to-markdown readme.txt README.md"
  },
  "devDependencies": {
    "onchange": "^7.1.0",
    "sass": "^1.77.0",
    "wp-readme-to-markdown": "^1.0.1"
  },
  "volta": {
    "node": "18.20.0"
  }
}
